          }     
        }, {          
          "name": "Measure Type",
          "type": "string",
          "dimension": true, 
          "constraints": {
            "enum": [
              "Number", 
              "Percent"
            ]
          }     
        }, {
          "name": "Variable",
          "type": "string",
          "dimension": true, 
          "constraints": {
